interface Bill {
    int calculate(int units);
}

class EB implements Bill {
    public int calculate(int units) {
        return units * 5;
    }
}

public class ElectricityBill {
    public static void main(String[] args) {
        Bill b = new EB();
        System.out.println("Bill Amount: â‚¹" + b.calculate(120));
    }
}
